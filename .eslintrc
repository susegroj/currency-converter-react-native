parser:
  babel-eslint

extends:
  eslint-config-airbnb

settings:
  ecmascript: 6

env:
  browser: true
  node: true
  mocha: true
  jest: true

rules:
  jsx: true
  modules: true
  destructuring: true
  classes: true
  forOf: true
  blockBindings: true
  arrowFunctions: true
  global-require: 0
  indent: 2
  func-style: 0
  func-names: 0
  comma-dangle: 0
  no-console: 0
  no-param-reassign: 0
  linebreak-style: 0
  import/no-extraneous-dependencies: 0
  import/no-unresolved: 0
  import/extensions: 0
  object-curly-newline: ["error", { "multiline": true }]
  "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  prop-types: 0
  react/forbid-prop-types: 0
  react/no-danger: 0
  jsx-ally/no-static-element-interactions: 0
  max-len: [1, 125, 2]
